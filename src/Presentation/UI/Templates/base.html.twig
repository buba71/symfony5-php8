                    <ul class="navbar__links">
                        <li class="nav__item">
                            <a class="nav__link active" aria-current="page" href="#">Home</a>
                        </li>
                        <li class="nav__item">
                            <a class="nav__link" href="#">Link 1</a>
                        </li>
                        <li class="nav__item">
                            <a class="nav__link disabled" href="#">Link2</a>
                        </li>
                    </ul>
            </div>
        </nav>

    </header>
        {% block body %}{% endblock %}
